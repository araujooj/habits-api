{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-unused-vars": "off",
    "no-unused-expressions": "off",
    "class-methods-use-this": "off",
    "import/extensions": [
      "off",
      "ignorePackages",
      {
        "ts": "never"
      }
    ],
    "import/no-unresolved": "off",
    "no-extra-semi": "off",
    "no-empty-function": "off",
    "no-useless-constructor": "off",
    "semi": "off",
    "camelcase": "off",
    "operator-linebreak": "off",
    "indent": "off",
    "@typescript-eslint/indent": [
      "error",
      2
    ]
  },
  "settings": {}
}